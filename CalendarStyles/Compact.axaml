  <x:Double x:Key="MonthViewGridMinHeight">150</x:Double>
  <x:Double x:Key="YearViewGridMinHeight">150</x:Double>
  <x:Double x:Key="CalendarDayButtonMinWidth">20</x:Double>
  <x:Double x:Key="CalendarDayButtonMinHeight">20</x:Double>
  <x:Double x:Key="CalendarButtonMinWidth">10</x:Double>
  <x:Double x:Key="CalendarButtonMinHeight">10</x:Double>
  
