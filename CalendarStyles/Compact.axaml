<ResourceDictionary xmlns="https://github.com/avaloniaui"
                    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
                    x:ClassModifier="internal">

  <Design.PreviewWith>
      <Calendar />
  </Design.PreviewWith>
  <x:Double x:Key="HeadContentFontSize">11</x:Double>
  <x:Double x:Key="CalendarDayButtonFontSize">10</x:Double>
  <x:Double x:Key="CalendarMonthNameFontSize">11</x:Double>
  <x:Double x:Key="CalendarDayButtonContentFontSize">11</x:Double>
  <GridLength x:Key="HeadContentGridRowHeight">20</GridLength>
